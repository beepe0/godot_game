; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="godot_project"
run/main_scene="res://scenes/game.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
run/max_fps=60
config/icon="res://icon.svg"

[audio]

driver/enable_input=true

[autoload]

DrawGizmos="*res://submodules/debug_draw_system/DrawGizmos.cs"

[debug]

multirun/number_of_windows=2
multirun/add_custom_args=true
multirun/window_args=["listen", "join"]

[debug_drawing]

layers/layer_2="2"

[debug_gizmos_system]

target_pool_size=1024
start_pool_size=256
meshes/box_mesh_res="res://submodules/debug_draw_system/gizmos/meshes/gizmos_Box.res"
meshes/arrow_mesh_res="res://submodules/debug_draw_system/gizmos/meshes/gizmos_Arrow.res"
meshes/circle_mesh_res="res://submodules/debug_draw_system/gizmos/meshes/gizmos_Circle.res"
meshes/plane_mesh_res="res://submodules/debug_draw_system/gizmos/meshes/gizmos_Plane.res"
meshes/sphere_mesh_res="res://submodules/debug_draw_system/gizmos/meshes/gizmos_Sphere.res"
meshes/capsule_mesh_res="res://submodules/debug_draw_system/gizmos/meshes/gizmos_Capsule.res"
meshes/cylinder_mesh_res="res://submodules/debug_draw_system/gizmos/meshes/gizmos_Cylinder.res"
meshes/point_mesh_res="res://submodules/debug_draw_system/gizmos/meshes/gizmos_Point.res"
meshes/solid_box_mesh_res="res://submodules/debug_draw_system/gizmos/meshes/gizmos_SolidBox.res"
meshes/arrows_mesh_res="res://submodules/debug_draw_system/gizmos/meshes/gizmos_Arrows.res"
meshes/solid_capsule_mesh_res="res://submodules/debug_draw_system/gizmos/meshes/gizmos_SolidCapsule.res"
meshes/solid_cylinder_mesh_res="res://submodules/debug_draw_system/gizmos/meshes/gizmos_SolidCylinder.res"
meshes/solid_sphere_mesh_res="res://submodules/debug_draw_system/gizmos/meshes/gizmos_SolidSphere.res"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/size/resizable=false
window/subwindows/embed_subwindows=false
window/stretch/mode="viewport"
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="godot_project"

[editor_plugins]

enabled=PackedStringArray("res://addons/debug_gizmos_system/plugin.cfg")

